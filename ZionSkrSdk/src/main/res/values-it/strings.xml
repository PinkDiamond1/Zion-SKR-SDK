    <string name="backup_toolbar_sub_title_active_status">Attivo</string><!--Active-->
    <string name="backup_invitation_message_first_part">Ciao, \n\n ti ho indicato come contatto attendibile per la funzione Social Key Recovery del portafoglio Zion. \n Segui le istruzioni di seguito \n per aiutarmi a eseguire il backup dei dati di recupero.\n\n</string><!--Hi, \n\n I\'ve designated you as my trusted contact for the Zion wallet\'s Social Key Recovery feature. \n Please follow the instructions below to \n help me back up my recovery data.\n\n-->
    <string name="backup_invitation_message_second_part">1) Toccare il collegamento per scaricare e installare Zion:\n</string><!--1) Tap this link to download and install Zion:\n-->
    <string name="backup_invitation_message_third_part">2) Una volta terminato, ricordare di \n chiedere il codice di verifica.\n\n</string><!--2) When you\'re done, remember to \n ask me for the verification code.\n\n-->
    <string name="backup_invitation_message_fourth_part">Grazie!</string><!--Thanks!-->
    <string name="backup_trust_item_sub">Ora ultima verifica: %1$s</string><!--Last checked time: %1$s-->
    <string name="backup_trust_item_sub2">Toccare qui per generare il codice.</string><!--Tap here to generate code.-->
    <string name="backup_resend_button_title">Rinvia</string><!--Resend-->
    <string name="backup_bottom_request_show">Richieste dagli amici</string><!--Requests from friends-->
    <string name="backup_remove_toolbar_title">Rimuovi contatto attendibile</string><!--Remove trusted contact-->
    <string name="backup_remove_button_delete">Rimuovi(%1$d)</string><!--Remove(%1$d)-->
    <string name="backup_menu_remove_trust_title">Rimuovi contatto attendibile</string><!--Remove trusted contact-->
