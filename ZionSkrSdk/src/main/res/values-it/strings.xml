    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">Cosa è Social Key Recovery\?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery può aiutare a recuperare in modo comodo il portafoglio dai contatti attendibili o la famiglia.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Invitare da 3 a 5 persone a essere contatti attendibili che possono aiutare a recuperare il portafoglio.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">I contatti attendibili possono inviare un codice per aiutare a ripristinare il portafoglio.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Configura</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Non ora</a></string>
    <string name="backup_introduction_ok">OK</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Backup dell\'elenco contatti attendibili</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Per recuperare il portafoglio con Social Key Recovery, selezionare un account Google per il backup dei contatti attendibili. L\'elenco dei contatti attendibili sarà salvato in Google Drive.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Aggiungi account</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Annulla</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Continua</string><!--Continue-->

<!--Social Key Recovery -->
    <string name="backup_trusted_contact">Contatto attendibile</string><!--Trusted contact-->
    <string name="backup_null_contact">Ancora non sono stati aggiunti contatti attendibili.</string><!--You have\'t added any trusted contacts yet.-->
    <string name="backup_warning_title">Social Key Recovery sarà disponibile quando saranno presenti almeno 3 contatti attendibili (Max:5).</string><!--Social Key Recovery will be available when you have at least 3 trusted contacts (Max:5).-->
    <string name="backup_invitation_title">Invita amici a essere \n contatti attendibili</string><!--Invite friends to be your \n trusted contact-->
    <string name="backup_invitation_content">Invitare da 3 a 5 amici o famigliari a essere contatti attendibili e aiutare a recuperare il portafoglio.</string><!--Invite 3 to 5 friends or family to be trusted contacts and help recover your wallet.-->
