    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">Apakah itu Social Key Recovery\?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery boleh membantu anda memulihkan dompet anda daripada rakan atau ahli keluarga anda yang dipercayai dengan mudah.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Jemput 3 hingga 5 orang menjadi kenalan dipercayai yang boleh membantu memulihkan dompet anda.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Kenalan anda yang dipercayai boleh menghantar kod bagi membantu anda memulihkan dompet anda.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Sediakan</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Bukan sekarang</a></string>
    <string name="backup_introduction_ok">OK</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Sandarkan senarai kenalan anda yang dipercayai</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Untuk memulihkan dompet anda dengan Social Key Recovery, pilih akaun Google untuk menyandarkan kenalan anda yang dipercayai. Senarai kenalan anda yang dipercayai akan disimpan dalam Google Drive.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Tambah akaun</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Batal</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Teruskan</string><!--Continue-->

<!--Social Key Recovery -->
    <string name="backup_trusted_contact">Kenalan dipercayai</string><!--Trusted contact-->
    <string name="backup_null_contact">Anda belum menambah sebarang kenalan yang dipercayai lagi.</string><!--You have\'t added any trusted contacts yet.-->
    <string name="backup_warning_title">Social Key Recovery akan tersedia apabila anda mempunyai sekurang-kurangnya 3 kenalan yang dipercayai (Maks:5).</string><!--Social Key Recovery will be available when you have at least 3 trusted contacts (Max:5).-->
    <string name="backup_invitation_title">Jemput rakan menjadi \n kenalan anda yang dipercayai</string><!--Invite friends to be your \n trusted contact-->
    <string name="backup_invitation_content">Jemput 3 hingga 5 rakan atau ahli keluarga menjadi kenalan yang dipercayai dan bantu memulihkan dompet anda.</string><!--Invite 3 to 5 friends or family to be trusted contacts and help recover your wallet.-->
    <string name="backup_invitation_hi">Hai %1$s,</string><!--Hi %1$s,-->
    <string name="backup_invitation_sharing">Kongsi jemputan</string><!--Share invitation-->
    <string name="backup_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="backup_toolbar_sub_title_no_active_status">Tidak aktif</string><!--Not active-->
    <string name="backup_toolbar_sub_title_active_status">Aktif</string><!--Active-->
    <string name="backup_invitation_message_first_part">Hai, \n\n saya telah menetapkan anda sebagai kenalan saya yang dipercayai untuk ciri Social Key Recovery dompet Zion. \n Sila ikut arahan di bawah untuk \n membantu saya menyandarkan data pemulihan saya.\n\n</string><!--Hi, \n\n I\'ve designated you as my trusted contact for the Zion wallet\'s Social Key Recovery feature. \n Please follow the instructions below to \n help me back up my recovery data.\n\n-->
    <string name="backup_invitation_message_second_part">1) Ketik pautan ini untuk memuat turun dan memasang Zion:\n</string><!--1) Tap this link to download and install Zion:\n-->
    <string name="backup_invitation_message_third_part">2) Apabila anda selesai, jangan lupa untuk \n meminta kod pengesahan daripada saya.\n\n</string><!--2) When you\'re done, remember to \n ask me for the verification code.\n\n-->
    <string name="backup_invitation_message_fourth_part">Terima Kasih!</string><!--Thanks!-->
    <string name="backup_trust_item_sub">Masa terakhir diperiksa: %1$s</string><!--Last checked time: %1$s-->
    <string name="backup_trust_item_sub2">Ketik di sini untuk menghasilkan kod.</string><!--Tap here to generate code.-->
    <string name="backup_resend_button_title">Hantar semula</string><!--Resend-->
    <string name="backup_bottom_request_show">Permintaan daripada rakan-rakan</string><!--Requests from friends-->
    <string name="backup_remove_toolbar_title">Alih keluar kenalan yang dipercayai</string><!--Remove trusted contact-->
    <string name="backup_remove_button_delete">Alih keluar(%1$d)</string><!--Remove(%1$d)-->
    <string name="backup_menu_remove_trust_title">Alih keluar kenalan yang dipercayai</string><!--Remove trusted contact-->
    <string name="backup_menu_log_out_title">Log keluar</string>
    <string name="backup_pending_target_toolbar">Permintaan daripada rakan-rakan</string><!--Requests from friends--> 
    <string name="backup_pending_target_null">Tiada permintaan buat masa ini.</string><!--No requests at this time.--> 
    <string name="backup_backup_on_same_device">Sandaran pada peranti yang sama tidak disokong.</string><!--Back up on same device is not supported.-->

