<!--Verification UI Start-->
    <string name="ver_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_content_title">%1$s đã thêm bạn làm người liên hệ tin cậy</string><!--%1$s added you as a trusted contact-->
    <string name="ver_step1">Bước 1. Nhập tên của bạn và yêu cầu mã xác minh từ %1$s.</string><!--Step 1. Enter your name and get the verification code from %1$s.--> 
    <string name="ver_step2">Bước 2. Mã xác minh của bạn được tạo trên điện thoại của %1$s. Yêu cầu gửi mã và nhập mã vào.</string> 
    <string name="ver_enter_name_hint">Nhập tên của bạn</string><!--Enter your name--> 
    <string name="ver_request_btn_text">Yêu cầu mã</string><!--Request code-->
    <string name="ver_sharing_title">Chia sẻ mã này cho %1$s</string><!--Share this code to %1$s-->
    <string name="ver_sharing_remind_route">M: %1$s\nthông qua %2$s</string><!--M: %1$s\nvia %1$s-->
