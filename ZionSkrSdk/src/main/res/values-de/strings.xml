<!--Verification UI Start-->
    <string name="ver_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_content_title">%1$s hat Sie als zuverlässigen Kontakt hinzugefügt</string><!--%1$s added you as a trusted contact-->
    <string name="ver_step1">Schritt 1. Geben Sie Ihren Namen ein und fragen Sie %1$s nach dem Bestätigungscode.</string><!--Step 1. Enter your name and get the verification code from %1$s.-->
    <string name="ver_step2">Schritt 2. Ihr Bestätigungscode wird auf dem Telefon von %1$s generiert. Fragen Sie sie nach dem Code und geben Sie ihn ein.</string>
    <string name="ver_enter_name_hint">Geben Sie Ihren Namen ein</string><!--Enter your name--> 
    <string name="ver_request_btn_text">Code anfragen</string><!--Request code-->
    <string name="ver_sharing_title">Diesen Code mit %1$s teilen</string><!--Share this code to %1$s-->
    <string name="ver_sharing_remind_route">M: %1$s\nüber %2$s</string><!--M: %1$s\nvia %1$s-->
    <string name="ver_sharing_reject">ABLEHNEN</string><!--REJECT-->
    <string name="ver_notification_channel_name">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_notification_request_title">Neue Anfrage von %1$s</string><!--New request from %1$s-->
    <string name="ver_notification_request_content">Tippen Sie hier, um den Bestätigungscode zu sehen und ihn mit %1$s zu teilen.</string><!--Tap to see the verification code and share it with %1$s.-->
    <string name="ver_notification_request_button">BESTÄTIGUNGSCODE ANZEIGEN</string><!--SHOW VERIFICATION CODE-->
    <string name="ver_no_response_hint">Keine Antwort? Sie können eine weitere Anfrage in %1$s stellen</string>
    <string name="ver_verify">Bestätigen</string><!--Verify-->
    <string name="ver_ok_congratulation_title">Sie sind jetzt ein vertrauenswürdiger Kontakt für %1$s.</string><!--You are now a trusted contact for %1$s.-->
    <string name="ver_ok_congratulation_msg">Sie können %1$s jetzt bei der Wiederherstellung der Wallet helfen, wenn sie verloren geht oder gestohlen wird. Zion hat einen Teil seiner Wallet Wiederherstellungsdaten verschlüsselt und auf Ihrem Gerät gespeichert. Achten Sie darauf, dass Sie keine Zion oder Zion App Daten löschen, da diese sonst verloren gehen. Zion prüft regelmäßig bei %2$s, um sicherzustellen, dass die Wiederherstellungsdaten weiterhin gültig sind.</string><!--You can now help %1$s recover their wallet if it is lost or stolen. Zion has encrypted and saved part of their wallet recovery data to your device. Make sure not to delete Zion or Zion app data, otherwise this recovery data will be lost. Zion will check with %1$s periodically to verify the recovery data is still valid.-->
    <string name="ver_ok_done">Fertig</string><!--Done-->
    <string name="ver_pin_error_hint">Dieser Code ist ungültig, bitte versuchen Sie es erneut.</string><!--This code is invalid, please try again.-->
    <string name="ver_dialog_btn_ok">OK</string><!--OK-->
    <string name="ver_dialog_backup_done_title">Sie sind bereits der vertrauenswürdige Kontakt von %1$s</string><!--You are already %1$s’s trusted contact-->
    <string name="ver_dialog_backup_done_msg">Vielen Dank für Ihre Hilfe bei der Sicherung der Daten von %1$s</string><!--Thanks for helping back up %1$s’s data-->
    <string name="ver_dialog_verifying_title">Verifizierung gesendet</string><!--Verification sent-->
    <string name="ver_dialog_verifying">Warten auf die Verifizierung. Sie werden der vertrauenswürdige Kontakt von %1$s sein, wenn die Verifizierung abgeschlossen ist.</string><!--Waiting for verifying. You will be %1$s’s trusted contact when verification is completed.-->
    <string name="ver_unknown_phone_number">Unbekannt</string><!--Unknown-->
    <string name="ver_unknown_name">Unbekannt</string><!--Unknown-->
    <string name="ver_dialog_trusted_contact_full_title">Vielen Dank für deine Hilfe!</string><!--Thank you for helping out!-->
    <string name="ver_dialog_trusted_contact_full_content">%1$s hat bereits das Maximum von 5 vertrauenswürdigen Kontakten erreicht.</string><!--%1$s has already reached the maximum of 5 trusted contacts.-->
    <string name="ver_expired_link">Dieser Link ist abgelaufen, bitte Sie %1$s, den aktualisierten Link zu senden.</string><!--This link is expired, please ask %1$s to send the updated link.-->
    <string name="ver_request_timeout">Zeitüberschreitung der Anfrage, bitte versuchen Sie es später noch einmal.</string><!--Request timeout, please try again later.-->
<!--Verification UI End-->

    <string name="try_too_often_title">Versuchen Sie es später noch einmal</string><!--Please try again later-->
