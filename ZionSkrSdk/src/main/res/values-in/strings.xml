    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">Apa itu Social Key Recovery\?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery dapat membantu Anda secara praktis memulihkan dompet Anda dari teman tepercaya atau keluarga.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Undang 3 hingga 5 orang untuk menjadi kontak tepercaya yang dapat membantu memulihkan dompet Anda.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Kontak tepercaya Anda dapat mengirimkan kode untuk membantu Anda memulihkan dompet Anda.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Penyiapan</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Tidak sekarang</a></string>
    <string name="backup_introduction_ok">OK</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Cadangkan daftar kontak tepercaya Anda</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Untuk memulihkan dompet Anda dengan Social Key Recovery, pilih akun Google untuk mencadangkan kontak tepercaya. Daftar kontak tepercaya Anda akan disimpan di Google Drive.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Tambahkan akun</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Batal</string><!--Cancel-->
