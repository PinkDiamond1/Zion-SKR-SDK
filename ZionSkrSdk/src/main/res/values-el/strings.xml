    <string name="ver_dialog_verifying_title">Η επαλήθευση έχει αποσταλεί</string><!--Verification sent-->
    <string name="ver_dialog_verifying">Αναμονή για επαλήθευση. Θα είστε αξιόπιστη επαφή του/της %1$s όταν ολοκληρωθεί η επαλήθευση.</string><!--Waiting for verifying. You will be %1$s’s trusted contact when verification is completed.-->
    <string name="ver_unknown_phone_number">Unknown</string><!--Unknown-->
    <string name="ver_unknown_name">Unknown</string><!--Unknown-->
