<!--Verification UI Start-->
    <string name="ver_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_content_title">%1$s heeft je toegevoegd als vertrouwde contactpersoon</string><!--%1$s added you as a trusted contact-->
    <string name="ver_step1">Stap 1. Voer je naam in en ontvang de verificatiecode van %1$s.</string><!--Step 1. Enter your name and get the verification code from %1$s.--> 
    <string name="ver_step2">Stap 2. Je verificatiecode is gegenereerd op de telefoon van %1$s. Vraag ze om de code en voer het in.</string> 
