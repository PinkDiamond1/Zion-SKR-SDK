    <string name="ver_notification_request_title">Nieuw verzoek van %1$s</string><!--New request from %1$s--> 
    <string name="ver_notification_request_content">Tik om de verificatiecode te zien en het te delen met %1$s.</string><!--Tap to see the verification code and share it with %1$s.-->
