<!--Verification UI Start-->
    <string name="ver_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_content_title">נוספת לרשימת אנשי הקשר לביטחון של %1$s</string><!--%1$s added you as a trusted contact-->
    <string name="ver_step1">שלב 1. להזין את השם ולבקש את קוד האימות של s%.</string><!--Step 1. Enter your name and get the verification code from %1$s.--> 
    <string name="ver_step2">שלב 2. קוד האימות נוצר בטלפון של s%. צריך לבקש את הקוד ולהזין אותו.</string> 
    <string name="ver_enter_name_hint">איך קוראים לך?</string><!--Enter your name--> 
    <string name="ver_request_btn_text">בקשת קוד</string><!--Request code-->
    <string name="ver_sharing_title">שיתוף הקוד עם s%</string><!--Share this code to %1$s-->
    <string name="ver_sharing_remind_route">נייד: %2$s דרך %1$s\n</string><!--M: %1$s\nvia %1$s-->
    <string name="ver_sharing_reject">דחייה</string><!--REJECT-->
    <string name="ver_notification_channel_name">מפתח שחזור חברתי</string><!--Social Key Recovery-->
    <string name="ver_notification_request_title">בקשה חדשה של %1$s</string><!--New request from %1$s--> 
    <string name="ver_notification_request_content">אפשר להקיש כדי להציג את קוד האימות ולשתף אותו עם s%.</string><!--Tap to see the verification code and share it with %1$s.-->
    <string name="ver_notification_request_button">הצגת קוד האימות</string><!--SHOW VERIFICATION CODE-->
    <string name="ver_no_response_hint">אין תגובה? אפשר יהיה לשלוח בקשה נוספת בעוד s%</string>
    <string name="ver_verify">אימות</string><!--Verify-->
    <string name="ver_ok_congratulation_title">צורפת לרשימת אנשי הקשר לביטחון של %1$s.</string><!--You are now a trusted contact for %1$s.--> 
    <string name="ver_ok_congratulation_msg">מעכשיו יש לך אפשרות לעזור בשחזור הארנק של %2$s אם המכשיר יאבד או ייגנב. חלק מהנתונים של הארנק הזה הוצפנו על ידי Zion ונשמרו במכשיר שלך כדי שיהיה אפשר לשחזר אותו. חשוב לא למחוק את אפליקציית Zion או את הנתונים של Zion כדי שנתוני השחזור לא יימחקו. אפליקציית Zion תבדוק מול %1$s מדי פעם שנתוני הגיבוי עדיין בתוקף.</string><!--You can now help %1$s recover their wallet if it is lost or stolen. Zion has encrypted and saved part of their wallet recovery data to your device. Make sure not to delete Zion or Zion app data, otherwise this recovery data will be lost. Zion will check with %1$s periodically to verify the recovery data is still valid.-->
    <string name="ver_ok_done">בוצע</string><!--Done-->
    <string name="ver_pin_error_hint">הקוד הזה לא תקין. אפשר לנסות שוב.</string><!--This code is invalid, please try again.-->
    <string name="ver_dialog_btn_ok">אישור</string><!--OK-->
    <string name="ver_dialog_backup_done_title">כבר נוספת לרשימת אנשי הקשר לביטחון של %1$s</string><!--You are already %1$s’s trusted contact-->
    <string name="ver_dialog_backup_done_msg">תודה על העזרה בגיבוי המידע של %1$s</string><!--Thanks for helping back up %1$s’s data-->
    <string name="ver_dialog_verifying_title">הודעת האימות נשלחה</string><!--Verification sent-->
    <string name="ver_dialog_verifying">בהמתנה לאימות. נוסיף אותך לרשימת אנשי הקשר לביטחון של s% אחרי האימות.</string><!--Waiting for verifying. You will be %1$s’s trusted contact when verification is completed.-->
    <string name="ver_unknown_phone_number">לא ידוע</string><!--Unknown-->
    <string name="ver_unknown_name">לא ידוע</string><!--Unknown-->
    <string name="ver_dialog_trusted_contact_full_title">תודה על העזרה!</string><!--Thank you for helping out!-->
    <string name="ver_dialog_trusted_contact_full_content">ברשימה של %1$s כבר יש 5 אנשי קשר לביטחון, ולא יכולים להיות בה אנשים נוספים.</string><!--%1$s has already reached the maximum of 5 trusted contacts.-->
    <string name="ver_expired_link">פג התוקף של הקישור הזה. צריך לבקש מהחבר/ה (%1$s) לשלוח את הקישור המעודכן.</string><!--This link is expired, please ask %1$s to send the updated link.-->
    <string name="ver_request_timeout">פג תוקף הבקשה. אפשר לנסות שוב מאוחר יותר.</string><!--Request timeout, please try again later.-->
<!--Verification UI End-->

