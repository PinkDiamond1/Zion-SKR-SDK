    <string name="try_too_often_message">בוצעו יותר מדי ניסיונות ברציפות. אפשר לנסות שוב בעוד %1$d דקות</string>

    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">מה זה Social Key Recovery?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">באמצעות Social Key Recovery אפשר להיעזר בחברים או בקרובי משפחה כדי להיכנס מחדש לארנק בקלות במידת הצורך.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">אפשר להזמין 3-5 אנשים כאנשי קשר לביטחון. האנשים האלה יוכלו לעזור לך להיכנס לחשבון בארנק במידת הצורך.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">אנשי הקשר לביטחון יכולים לשלוח לך קוד כדי לעזור לך להיכנס לארנק.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">הגדרה</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">לא עכשיו</a></string>
    <string name="backup_introduction_ok">אישור</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">גיבוי הרשימה של אנשי הקשר לביטחון</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">כדי לשחזר את הארנק באמצעות Social Key Recovery, צריך לבחור חשבון Google שבו יישמרו אנשי קשר לביטחון. הרשימה של אנשי הקשר לביטחון תישמר ב-Google Drive.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">הוספת חשבון</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">ביטול</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">המשך</string><!--Continue-->

<!--Social Key Recovery -->
    <string name="backup_trusted_contact">אנשי קשר לביטחון</string><!--Trusted contact-->
    <string name="backup_null_contact">אין לך אף איש קשר לביטחון.</string><!--You have\'t added any trusted contacts yet.-->
    <string name="backup_warning_title">יהיה אפשר להיעזר ב-Social Key Recovery כשיהיו\nלפחות 3 אנשי קשר לביטחון (אפשר מקסימום 5).</string><!--Social Key Recovery will be available when you have at least 3 trusted contacts (Max:5).-->
    <string name="backup_invitation_title">הזמנת חברים\nכאנשי קשר לביטחון</string><!--Invite friends to be your \n trusted contact-->
    <string name="backup_invitation_content">אפשר להזמין 3-5 חברים כאנשי קשר לביטחון. החברים האלה יוכלו לעזור לך להיכנס לחשבון בארנק במידת הצורך.</string><!--Invite 3 to 5 friends or family to be trusted contacts and help recover your wallet.-->
    <string name="backup_invitation_hi">שלום %1$s,</string><!--Hi %1$s,-->
    <string name="backup_invitation_sharing">שיתוף ההזמנה</string><!--Share invitation-->
    <string name="backup_toolbar_title">מפתח שחזור חברתי</string><!--Social Key Recovery-->
    <string name="backup_toolbar_sub_title_no_active_status">לא פעיל</string><!--Not active-->
    <string name="backup_toolbar_sub_title_active_status">פעיל</string><!--Active-->
    <string name="backup_invitation_message_first_part">היי, \n\nהוספתי אותך לרשימת אנשי הקשר לביטחון שלי, כחלק מהתכונה Social Key Recovery בארנק שלי ב-Zion. \n כדי לעזור לי לגבות את הנתונים,\n צריך לבצע את הפעולות הבאות.\n\n</string><!--Hi, \n\n I\'ve designated you as my trusted contact for the Zion wallet\'s Social Key Recovery feature. \n Please follow the instructions below to \n help me back up my recovery data.\n\n-->
    <string name="backup_invitation_message_second_part">1) להקיש על הקישור הזה כדי להוריד ולהתקין את Zion:‏\n</string><!--1) Tap this link to download and install Zion:\n-->
    <string name="backup_invitation_message_third_part">2) בסיום, חשוב לזכור לבקש ממני\n את קוד האימות.\n\n</string><!--2) When you\'re done, remember to \n ask me for the verification code.\n\n-->
    <string name="backup_invitation_message_fourth_part">תודה!</string><!--Thanks!-->
    <string name="backup_trust_item_sub">מועד בדיקה אחרונה: %1$s</string><!--Last checked time: %1$s-->
    <string name="backup_trust_item_sub2">אפשר להקיש כאן כדי ליצור קוד.</string><!--Tap here to generate code.-->
    <string name="backup_resend_button_title">שליחה מחדש</string><!--Resend-->
    <string name="backup_bottom_request_show">בקשות מחברים</string><!--Requests from friends-->
    <string name="backup_remove_toolbar_title">הסרה מאנשי הקשר לביטחון</string><!--Remove trusted contact-->
    <string name="backup_remove_button_delete">הסרה (%1$d)</string><!--Remove(%1$d)-->
    <string name="backup_menu_remove_trust_title">הסרה מאנשי הקשר לביטחון</string><!--Remove trusted contact-->
    <string name="backup_menu_log_out_title">התנתקות</string>
    <string name="backup_pending_target_toolbar">בקשות מחברים</string><!--Requests from friends--> 
