<!--Verification UI Start-->
    <string name="ver_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_content_title">%1$s felvette Önt megbízható partnerként</string><!--%1$s added you as a trusted contact-->
    <string name="ver_step1">1. lépés Adja meg a nevét, valamint kérje el az ellenőrző kódot (forrás: %1$s).</string><!--Step 1. Enter your name and get the verification code from %1$s.-->
    <string name="ver_step2">2. lépés %1$s telefonján előáll az Ön ellenőrző kódja. Kérje el tőle a kódot, majd írja be.</string>
    <string name="ver_enter_name_hint">Adja meg a nevét</string><!--Enter your name--> 
    <string name="ver_request_btn_text">Kód kérése</string><!--Request code-->
    <string name="ver_sharing_title">Kód megosztása a következővel: %1$s</string><!--Share this code to %1$s-->
    <string name="ver_sharing_remind_route">M: %1$s\na következőn csatornán: %2$s</string><!--M: %1$s\nvia %1$s-->
    <string name="ver_sharing_reject">VISSZAUTASÍTÁS</string><!--REJECT-->
    <string name="ver_notification_channel_name">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_notification_request_title">Új kérés a következőtől: %1$s</string><!--New request from %1$s-->
    <string name="ver_notification_request_content">Érintse meg az ellenőrző kód megjelenítéséhez és megosztásához (a megosztás címzettje: %1$s).</string><!--Tap to see the verification code and share it with %1$s.-->
    <string name="ver_notification_request_button">ELLENŐRZŐ KÓD MEGJELENÍTÉSE</string><!--SHOW VERIFICATION CODE-->
    <string name="ver_no_response_hint">Nincs válasz? %1$s múlva újabb kérést is indíthat</string>
    <string name="ver_verify">Ellenőrzés</string><!--Verify-->
    <string name="ver_ok_congratulation_title">Ön %1$s megbízható partnerévé vált.</string><!--You are now a trusted contact for %1$s.-->
    <string name="ver_ok_congratulation_msg">Készen áll arra, hogy %1$s segítségére legyen a tárca visszaállításában, ha elveszíti vagy ellopják azt. A Zion (titkosítva) mentette a tárcájuk visszaállításához szükséges adatok egy részét az Ön készülékére. Ügyeljen arra, hogy ne törölje a Zion alkalmazást vagy a hozzá tartozó adatokat, mert ebben az esetben a visszaállítási adatok elvesznek. A Zion rendszeres időközönként ellenőrizni fogja, hogy %2$s visszaállítási adatai továbbra is érvényesek-e.</string><!--You can now help %1$s recover their wallet if it is lost or stolen. Zion has encrypted and saved part of their wallet recovery data to your device. Make sure not to delete Zion or Zion app data, otherwise this recovery data will be lost. Zion will check with %1$s periodically to verify the recovery data is still valid.-->
    <string name="ver_ok_done">Kész</string><!--Done-->
    <string name="ver_pin_error_hint">A kód érvénytelen, próbálja meg újra.</string><!--This code is invalid, please try again.-->
    <string name="ver_dialog_btn_ok">OK</string><!--OK-->
    <string name="ver_dialog_backup_done_title">Már szerepel %1$s megbízható partnerei között</string><!--You are already %1$s’s trusted contact-->
    <string name="ver_dialog_backup_done_msg">Köszönjük a segítségét %1$s adatainak biztonsági mentésében</string><!--Thanks for helping back up %1$s’s data-->
    <string name="ver_dialog_verifying_title">Megerősítés elküldve</string><!--Verification sent-->
    <string name="ver_dialog_verifying">Várakozás a megerősítésre. A megerősítést követően Ön %1$s megbízható partnere lesz.</string><!--Waiting for verifying. You will be %1$s’s trusted contact when verification is completed.-->
    <string name="ver_unknown_phone_number">Ismeretlen</string><!--Unknown-->
    <string name="ver_unknown_name">Ismeretlen</string><!--Unknown-->
    <string name="ver_dialog_trusted_contact_full_title">Köszönöm a segítséget!</string><!--Thank you for helping out!-->
    <string name="ver_dialog_trusted_contact_full_content">%1$s már elérte a megbízható partnerek számát (5).</string><!--%1$s has already reached the maximum of 5 trusted contacts.-->
    <string name="ver_expired_link">A hivatkozás lejárt; kérje meg a feladót (%1$s), hogy küldjön egy frissített hivatkozást.</string><!--This link is expired, please ask %1$s to send the updated link.-->
    <string name="ver_request_timeout">Időtúllépés történt a kérés során, próbálja meg újra később.</string><!--Request timeout, please try again later.-->
