    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">Mi a Social Key Recovery\?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">A Social Key Recovery segítségével kényelmesen visszaállíthatja a tárcáját a megbízható partnereinek és a családtagjainak segítségével.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Hívja meg 3-5 ismerősét megbízható partnernek; ők segíthetnek visszaállítani a tárcáját.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">A megbízható partnerek egy kód elküldésével segíthetnek a tárca visszaállításában.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Beállítás</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Most nem</a></string>
    <string name="backup_introduction_ok">OK</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Megbízható partnerek listájának biztonsági mentése</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Ha vissza szeretné állítani a tárcát a Social Key Recovery funkcióval, akkor válassza ki azt a Google fiókot, amelybe menteni kívánja a megbízható partnereket. A megbízható partnerek listája a Google Drive-on fog tárolódni.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Fiók hozzáadása</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Mégse</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Folytatás</string><!--Continue-->

<!--Social Key Recovery -->
