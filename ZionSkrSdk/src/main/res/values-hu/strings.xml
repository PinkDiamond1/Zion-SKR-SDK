    <string name="backup_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="backup_toolbar_sub_title_no_active_status">Nem aktív</string><!--Not active-->
    <string name="backup_toolbar_sub_title_active_status">Aktív</string><!--Active-->
    <string name="backup_invitation_message_first_part">Szia! \n\n Megjelöltelek megbízható partneremként a Zion tárcám Social Key Recovery funkciójában. \n Kérlek, az alábbi útmutatást követve segíts \n biztonsági mentést készíteni a visszaállítási adataimról.\n\n</string><!--Hi, \n\n I\'ve designated you as my trusted contact for the Zion wallet\'s Social Key Recovery feature. \n Please follow the instructions below to \n help me back up my recovery data.\n\n-->
    <string name="backup_invitation_message_second_part">1) A hivatkozást megérintve töltsd le és telepítsd a Zion alkalmazást:\n</string><!--1) Tap this link to download and install Zion:\n-->
    <string name="backup_invitation_message_third_part">2) Amikor végeztél, kérd el tőlem \n az ellenőrző kódot.\n\n</string><!--2) When you\'re done, remember to \n ask me for the verification code.\n\n-->
    <string name="backup_invitation_message_fourth_part">Köszönöm!</string><!--Thanks!-->
    <string name="backup_trust_item_sub">Utolsó ellenőrzés időpontja: %1$s</string><!--Last checked time: %1$s-->
    <string name="backup_trust_item_sub2">Érintse meg itt a kód előállításához.</string><!--Tap here to generate code.-->
    <string name="backup_resend_button_title">Újraküldés</string><!--Resend-->
    <string name="backup_bottom_request_show">Ismerősöktől érkezett kérések</string><!--Requests from friends-->
    <string name="backup_remove_toolbar_title">Megbízható partner eltávolítása</string><!--Remove trusted contact-->
    <string name="backup_remove_button_delete">Eltávolítás (%1$d)</string><!--Remove(%1$d)-->
    <string name="backup_menu_remove_trust_title">Megbízható partner eltávolítása</string><!--Remove trusted contact-->
    <string name="backup_menu_log_out_title">Kijelentkezés</string>
    <string name="backup_pending_target_toolbar">Ismerősöktől érkezett kérések</string><!--Requests from friends--> 
    <string name="backup_pending_target_null">Jelenleg nincs kérés.</string><!--No requests at this time.--> 
    <string name="backup_backup_on_same_device">Azonos készülékre nem készíthető biztonsági mentés.</string><!--Back up on same device is not supported.-->

