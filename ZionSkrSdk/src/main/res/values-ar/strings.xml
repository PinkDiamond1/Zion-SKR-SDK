    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">ما المقصود بخدمة استعادة المفتاح المشترك Social Key Recovery\؟</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">تساعدك خدمة استعادة المفتاح المشترك Social Key Recovery على استعادة محفظتك من أصدقائك الموثوق فيهم أو العائلة الموثوق فيها بطريقة سهلة.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">وجِّه الدعوة إلى 3 إلى 5 أشخاص للانضمام إلى جهات الاتصال التي تثق فيها والتي يمكنها مساعدتك على لاستعادة محفظتك.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">باستطاعة جهات الاتصال الموثوق فيها إرسال رمز لمساعدتك على استعادة محفظتك.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">إعداد</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">ليس الآن</a></string>
    <string name="backup_introduction_ok">موافق</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">انسخ احتياطيًا قائمة جهات الاتصال الموثوق فيها</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">لاستعادة محفظتك عبر خدمة استعادة المفتاح المشترك Social Key Recovery، حدد حساب Google لإجراء نسخ احتياطي لجهات الاتصال الموثوق فيها. سيتم تخزين قائمة جهات الاتصال الموثوق فيها في Google Drive.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">إضافة حساب</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">إلغاء</string><!--Cancel-->
