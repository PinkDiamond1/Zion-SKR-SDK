<!--Verification UI Start-->
    <string name="ver_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_content_title">%1$s lisas teid usaldusväärseks kontaktiks</string><!--%1$s added you as a trusted contact-->
    <string name="ver_step1">1. samm. Sisestage oma nimi ja hankige kasutajalt %1$s kinnituskood.</string><!--Step 1. Enter your name and get the verification code from %1$s.-->
    <string name="ver_step2">2. samm. Teie kinnituskood luuakse kasutaja %1$s telefonis. Küsige talt koodi ja sisestage see.</string>
    <string name="ver_enter_name_hint">Sisestage oma nimi</string><!--Enter your name-->
    <string name="ver_request_btn_text">Taotle koodi</string><!--Request code-->
    <string name="ver_sharing_title">Jagage seda koodi kasutajaga %1$s</string><!--Share this code to %1$s-->
    <string name="ver_sharing_remind_route">M: %1$s\nteenuse %2$s kaudu</string><!--M: %1$s\nvia %1$s-->
    <string name="ver_sharing_reject">KEELDU</string><!--REJECT-->
    <string name="ver_notification_channel_name">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_notification_request_title">Uus taotlus kasutajalt %1$s</string><!--New request from %1$s-->
    <string name="ver_notification_request_content">Toksake, et näha kinnituskoodi ja jagada seda kasutajaga %1$s.</string><!--Tap to see the verification code and share it with %1$s.-->
    <string name="ver_notification_request_button">KUVA KINNITUSKOOD</string><!--SHOW VERIFICATION CODE-->
    <string name="ver_no_response_hint">Kas te ei saanud vastust? Saate uue taotluse esitada, kui möödunud on %1$s</string>
    <string name="ver_verify">Kinnita</string><!--Verify-->
    <string name="ver_ok_congratulation_title">Olete nüüd kasutaja %1$s usaldusväärne kontakt.</string><!--You are now a trusted contact for %1$s.-->
    <string name="ver_ok_congratulation_msg">Nüüd saate aidata kasutajal %1$s tema rahakotti taastada, kui see varastatakse või kaduma läheb. Zion krüptis osa rahakoti taastamise andmetest ja salvestas need teie seadmesse. Ärge kustutage rakendust Zion ega selle rakenduse andmeid, kuna need taastamisandmed läheksid kaduma. Zion kontrollib kasutajalt %2$s regulaarselt, et veenduda, et taastamise andmed kehtivad veel.</string><!--You can now help %1$s recover their wallet if it is lost or stolen. Zion has encrypted and saved part of their wallet recovery data to your device. Make sure not to delete Zion or Zion app data, otherwise this recovery data will be lost. Zion will check with %1$s periodically to verify the recovery data is still valid.-->
    <string name="ver_ok_done">Valmis</string><!--Done-->
    <string name="ver_pin_error_hint">See kood on vale. Proovige uuesti.</string><!--This code is invalid, please try again.-->
    <string name="ver_dialog_btn_ok">OK</string><!--OK-->
    <string name="ver_dialog_backup_done_title">Olete juba kasutaja %1$s usaldusväärne kontakt</string><!--You are already %1$s’s trusted contact-->
    <string name="ver_dialog_backup_done_msg">Aitäh abi eest kasutaja %1$s andmete varundamisel</string><!--Thanks for helping back up %1$s’s data-->
    <string name="ver_dialog_verifying_title">Kinnitus on saadetud</string><!--Verification sent-->
    <string name="ver_dialog_verifying">Kinnituse ootel. Olete kasutaja %1$s usaldusväärne kontakt, kui kinnitamine on lõpetatud.</string><!--Waiting for verifying. You will be %1$s’s trusted contact when verification is completed.-->
    <string name="ver_unknown_phone_number">Tundmatu</string><!--Unknown-->
    <string name="ver_unknown_name">Tundmatu</string><!--Unknown-->
    <string name="ver_dialog_trusted_contact_full_title">Aitäh abi eest!</string><!--Thank you for helping out!-->
    <string name="ver_dialog_trusted_contact_full_content">%1$s on juba saavutanud maksimaalsed 5 usaldusväärset kontakti.</string><!--%1$s has already reached the maximum of 5 trusted contacts.-->
    <string name="ver_expired_link">See link on aegunud. Paluge kasutajal %1$s uus link saata.</string><!--This link is expired, please ask %1$s to send the updated link.-->
    <string name="ver_request_timeout">Taotlus aegus. Proovige hiljem uuesti.</string><!--Request timeout, please try again later.-->
