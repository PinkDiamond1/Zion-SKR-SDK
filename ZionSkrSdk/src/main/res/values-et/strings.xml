    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">Mis on Social Key Recovery\?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery aitab teil usaldusväärsete sõprade või pereliikmete abiga mugavalt rahakoti taastada.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Saate saata 3–5 inimesele usaldusväärse kontakti kutse, et nad saaksid aidata teil rahakotti taastada.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.-->
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Usaldusväärsed kontaktid saavad koodi saata, et aidata teil rahakotti taastada.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Seadista</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Mitte praegu</a></string>
    <string name="backup_introduction_ok">OK</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Usaldusväärsete kontaktide loendi varundamine</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Rahakoti taastamiseks Social Key Recovery kaudu valige Google\'i konto, kuhu oma usaldusväärsed kontaktid varundada. Teie usaldusväärsete kontaktide loend salvestatakse Google Drive\'i.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Lisa konto</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Tühista</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Jätka</string><!--Continue-->

<!--Social Key Recovery -->
    <string name="backup_trusted_contact">Usaldusväärne kontakt</string><!--Trusted contact-->
    <string name="backup_null_contact">Te ei ole veel usaldusväärseid kontakte lisanud.</string><!--You have\'t added any trusted contacts yet.-->
    <string name="backup_warning_title">Social Key Recovery on saadaval, kui teil on vähemalt 3 usaldusväärset kontakti (max 5).</string><!--Social Key Recovery will be available when you have at least 3 trusted contacts (Max:5).-->
    <string name="backup_invitation_title">Saatke sõpradele usaldusväärse \n kontakti kutse</string><!--Invite friends to be your \n trusted contact-->
    <string name="backup_invitation_content">Saate saata 3–5 sõbrale või pereliikmele usaldusväärse kontakti kutse, et nad saaksid aidata teil rahakotti taastada.</string><!--Invite 3 to 5 friends or family to be trusted contacts and help recover your wallet.-->
    <string name="backup_invitation_hi">Tere, %1$s</string><!--Hi %1$s,-->
    <string name="backup_invitation_sharing">Jaga kutset</string><!--Share invitation-->
    <string name="backup_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="backup_toolbar_sub_title_no_active_status">Pole aktiivne</string><!--Not active-->
    <string name="backup_toolbar_sub_title_active_status">Aktiivne</string><!--Active-->
    <string name="backup_invitation_message_first_part">Hei! \n\n Määrasin su Zioni rahakoti Social Key Recovery funktsiooni puhul enda usaldusväärseks kontaktiks. \n Palun järgi allolevaid suuniseid, \n et aidata mul taasteandmed varundada.\n\n</string><!--Hi, \n\n I\'ve designated you as my trusted contact for the Zion wallet\'s Social Key Recovery feature. \n Please follow the instructions below to \n help me back up my recovery data.\n\n-->
    <string name="backup_invitation_message_second_part">1) Toksa järgmisel lingil, et Zion alla laadida ja installida:\n</string><!--1) Tap this link to download and install Zion:\n-->
    <string name="backup_invitation_message_third_part">2) Kui oled lõpetanud, ära unusta \n mult kinnituskoodi küsida.\n\n</string><!--2) When you\'re done, remember to \n ask me for the verification code.\n\n-->
    <string name="backup_invitation_message_fourth_part">Aitäh!</string><!--Thanks!-->
    <string name="backup_trust_item_sub">Viimase kontrolli aeg: %1$s</string><!--Last checked time: %1$s-->
    <string name="backup_trust_item_sub2">Toksake siin, et kood luua.</string><!--Tap here to generate code.-->
    <string name="backup_resend_button_title">Saada uuesti</string><!--Resend-->
    <string name="backup_bottom_request_show">Taotlused sõpradelt</string><!--Requests from friends-->
    <string name="backup_remove_toolbar_title">Usaldusväärse kontakti eemaldamine</string><!--Remove trusted contact-->
    <string name="backup_remove_button_delete">Eemalda (%1$d)</string><!--Remove(%1$d)-->
    <string name="backup_menu_remove_trust_title">Usaldusväärse kontakti eemaldamine</string><!--Remove trusted contact-->
