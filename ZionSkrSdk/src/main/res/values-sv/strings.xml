<!--Verification UI Start-->
    <string name="ver_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_content_title">%1$s lade till dig som en betrodd kontakt</string><!--%1$s added you as a trusted contact-->
    <string name="ver_step1">Steg 1. Skriv in ditt namn och få verifieringskoden från %1$s.</string><!--Step 1. Enter your name and get the verification code from %1$s.--> 
    <string name="ver_step2">Steg 2. Din verifieringskod genereras på %1$ss telefon. Fråga dem om koden och skriv in den.</string> 
    <string name="ver_enter_name_hint">Ange ditt namn</string><!--Enter your name--> 
    <string name="ver_request_btn_text">Förfråga kod</string><!--Request code-->
    <string name="ver_sharing_title">Dela den här koden med %1$s</string><!--Share this code to %1$s-->
    <string name="ver_sharing_remind_route">M: %1$s genom %2$s\n</string><!--M: %1$s\nvia %1$s-->
