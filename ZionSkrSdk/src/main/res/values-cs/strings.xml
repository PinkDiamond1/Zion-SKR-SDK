    <string name="ver_unknown_name">Neznámé</string><!--Unknown-->
    <string name="ver_dialog_trusted_contact_full_title">Děkuji za pomoc!</string><!--Thank you for helping out!-->
    <string name="ver_dialog_trusted_contact_full_content">%1$s již dosáhl maxima 5 důvěryhodných kontaktů.</string><!--%1$s has already reached the maximum of 5 trusted contacts.-->
    <string name="ver_expired_link">Doba platnosti odkazu vypršela, požádejte prosím %1$s o odeslání aktualizovaného odkazu.</string><!--This link is expired, please ask %1$s to send the updated link.-->
