    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">Co je Social Key Recovery?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery vám může pomoci pohodlně obnovit vaši peněženku od vašich důvěryhodných známých nebo rodinných příslušníků.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Pozvěte 3 až 5 osob, aby se staly důvěryhodnými kontakty, kteří mohou pomoci obnovit vaši peněženku.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Vaše důvěryhodné kontakty mohou odeslat kód, který vám pomůže obnovit vaši peněženku.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Nastavit</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Nyní ne</a></string>
    <string name="backup_introduction_ok">OK</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Zálohujte svůj seznam důvěryhodných kontaktů</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Pro obnovení vaší peněženky pomocí Social Key Recovery zvolte účet Google pro zálohování vašich důvěryhodných kontaktů. Váš seznam důvěryhodných kontaktů bude uložen na Google Drive.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Přidat účet</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Storno</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Pokračovat</string><!--Continue-->

<!--Social Key Recovery -->
