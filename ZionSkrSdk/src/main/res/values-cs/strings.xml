    <string name="backup_invitation_title">Pozvěte známé, aby se stali vaším \n důvěryhodným kontaktem</string><!--Invite friends to be your \n trusted contact-->
    <string name="backup_invitation_content">Pozvěte 3 až 5 známých nebo rodinných příslušníků, aby se stali důvěryhodnými kontakty a pomohli obnovit vaši peněženku.</string><!--Invite 3 to 5 friends or family to be trusted contacts and help recover your wallet.-->
    <string name="backup_invitation_hi">Dobrý den %1$s,</string><!--Hi %1$s,-->
    <string name="backup_invitation_sharing">Sdílet pozvání</string><!--Share invitation-->
    <string name="backup_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="backup_toolbar_sub_title_no_active_status">Neaktivní</string><!--Not active-->
    <string name="backup_toolbar_sub_title_active_status">Aktivovat</string><!--Active-->
    <string name="backup_invitation_message_first_part">Dobrý den, \n\n stanovil/a jsem vás jako svůj důvěryhodný kontakt pro funkci Social Key Recovery peněženky Zion. \n Řiďte se prosím pokyny níže \n pro zálohování dat pro obnovení.\n\n</string><!--Hi, \n\n I\'ve designated you as my trusted contact for the Zion wallet\'s Social Key Recovery feature. \n Please follow the instructions below to \n help me back up my recovery data.\n\n-->
    <string name="backup_invitation_message_second_part">1) Klepnutím na tento odkaz stáhněte a nainstalujte Zion:\n</string><!--1) Tap this link to download and install Zion:\n-->
    <string name="backup_invitation_message_third_part">2) Po dokončení mě nezapomeňte \n požádat o ověřovací kód.\n\n</string><!--2) When you\'re done, remember to \n ask me for the verification code.\n\n-->
    <string name="backup_invitation_message_fourth_part">Děkujeme!</string><!--Thanks!-->
    <string name="backup_trust_item_sub">Naposledy zaznamenaný čas: %1$s</string><!--Last checked time: %1$s-->
    <string name="backup_trust_item_sub2">Klepněte sem pro generování kódu.</string><!--Tap here to generate code.-->
    <string name="backup_resend_button_title">Odeslat znovu</string><!--Resend-->
    <string name="backup_bottom_request_show">Žádosti od známých</string><!--Requests from friends-->
    <string name="backup_remove_toolbar_title">Odstranit důvěryhodný kontakt</string><!--Remove trusted contact-->
