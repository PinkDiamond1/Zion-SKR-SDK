<!--Verification UI Start-->
    <string name="ver_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_content_title">%1$s v-a adăugat ca și contact de încredere</string><!--%1$s added you as a trusted contact-->
    <string name="ver_step1">Pasul 1. Introduceți numele dvs. și obțineți codul de verificare de la %1$s.</string><!--Step 1. Enter your name and get the verification code from %1$s.--> 
    <string name="ver_step2">Pasul 2. Codul dvs. de verificare este generat pe telefonul contactului %1$s. Solicitați-i codul și introduceți-l.</string> 
    <string name="ver_enter_name_hint">Introduceţi numele</string><!--Enter your name--> 
    <string name="ver_request_btn_text">Solicitare cod</string><!--Request code-->
    <string name="ver_sharing_title">Partajează acest cod pe %1$s</string><!--Share this code to %1$s-->
    <string name="ver_sharing_remind_route">M: %1$s\nprin %2$s</string><!--M: %1$s\nvia %1$s-->
    <string name="ver_sharing_reject">RESPINGERE</string><!--REJECT-->
    <string name="ver_notification_channel_name">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_notification_request_title">Solicitare nouă de la %1$s</string><!--New request from %1$s--> 
    <string name="ver_notification_request_content">Apăsați pentru a vizualiza codul de verificare și partajați-l cu %1$s.</string><!--Tap to see the verification code and share it with %1$s.-->
    <string name="ver_notification_request_button">AFIȘAȚI CODUL DE VERIFICARE</string><!--SHOW VERIFICATION CODE-->
    <string name="ver_no_response_hint">Fără răspuns? Vei putea trimite o altă solicitare în %1$s</string>
    <string name="ver_verify">Verificare</string><!--Verify-->
    <string name="ver_ok_congratulation_title">Acum sunteți un contact de încredere pentru %1$s.</string><!--You are now a trusted contact for %1$s.--> 
    <string name="ver_ok_congratulation_msg">Acum îl puteți ajuta pe %1$s să își recupereze portofelul în cazul în care acesta este pierdut sau furat. Zion a criptat și salvat pe dispozitivul dvs. o parte din datele de recuperare a portofelului. Vă rugăm să nu ștergeți Zion și nici datele aplicației Zion, altfel aceste date de recuperare se vor pierde. Zion va contacta periodic pe %2$s pentru a se asigura că datele de recuperare sunt în continuare valabile.</string><!--You can now help %1$s recover their wallet if it is lost or stolen. Zion has encrypted and saved part of their wallet recovery data to your device. Make sure not to delete Zion or Zion app data, otherwise this recovery data will be lost. Zion will check with %1$s periodically to verify the recovery data is still valid.-->
    <string name="ver_ok_done">Efectuat</string><!--Done-->
    <string name="ver_pin_error_hint">Acest cod este nevalid, încercați din nou.</string><!--This code is invalid, please try again.-->
    <string name="ver_dialog_btn_ok">OK</string><!--OK-->
    <string name="ver_dialog_backup_done_title">Deja ești contactul de încredere al lui %1$s</string><!--You are already %1$s’s trusted contact-->
    <string name="ver_dialog_backup_done_msg">Mulțumesc pentru ajutorul oferit la realizarea copiei de rezervă pentru datele lui %1$s</string><!--Thanks for helping back up %1$s’s data-->
    <string name="ver_dialog_verifying_title">Verificare trimisă</string><!--Verification sent-->
    <string name="ver_dialog_verifying">Se aşteaptă verificarea. La finalizarea verificării vei deveni contactul de încredere al lui %1$s.</string><!--Waiting for verifying. You will be %1$s’s trusted contact when verification is completed.-->
    <string name="ver_unknown_phone_number">Necunoscut</string><!--Unknown-->
    <string name="ver_unknown_name">Necunoscut</string><!--Unknown-->
    <string name="ver_dialog_trusted_contact_full_title">Vă mulţumim pentru ajutor!</string><!--Thank you for helping out!-->
    <string name="ver_dialog_trusted_contact_full_content">%1$s a atins deja numărul maxim de 5 contacte de încredere.</string><!--%1$s has already reached the maximum of 5 trusted contacts.-->
    <string name="ver_expired_link">Acest link a expirat; solicitați lui %1$s să vă trimită linkul actualizat.</string><!--This link is expired, please ask %1$s to send the updated link.-->
    <string name="ver_request_timeout">Solicitarea a expirat; încercaţi din nou mai târziu.</string><!--Request timeout, please try again later.-->
