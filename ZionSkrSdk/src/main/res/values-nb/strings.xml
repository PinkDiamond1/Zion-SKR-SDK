    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">Hva er Social Key Recovery\?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery kan hjelpe deg med å gjenopprette lommeboken på en praktisk måte fra klarerte venner eller familie.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Inviter 3 til 5 personer som klarerte kontakter som kan hjelpe deg med å gjenopprette lommeboken din.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Dine klarerte kontakter kan sende deg en kode som hjelp til å gjenopprette lommeboken.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Sett opp</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Ikke nå</a></string>
    <string name="backup_introduction_ok">OK</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Sikkerhetskopier data om dine klarerte kontakter</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Når du skal gjenopprette lommeboken med Social Key Recovery, velger du en Google-konto for å sikkerhetskopiere dine klarerte kontakter. Listen over klarerte kontakter vil lagres i Google Drive.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Legg til konto</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Avbryt</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Fortsett</string><!--Continue-->

