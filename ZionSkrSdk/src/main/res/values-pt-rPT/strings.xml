    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">O que é a Social Key Recovery\?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery pode ajudar a recuperar facilmente a sua carteira usando amigos ou familiares de confiança.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Convide 3 a 5 pessoas para contactos de confiança que podem ajudar a recuperar a sua carteira.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Os seus contactos de confiança podem enviar um código para ajudar a restaurar a sua carteira.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Configurar</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Agora não</a></string>
    <string name="backup_introduction_ok">OK</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Salvaguarde a sua lista de contactos de confiança</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Para recuperar a sua carteira com Social Key Recovery, selecione uma conta para salvaguardar os seus contactos de confiança. A sua lista de contactos de confiança será armazenada na Google Drive.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Adicionar conta</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Cancelar</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Continuar</string><!--Continue-->

<!--Social Key Recovery -->
    <string name="backup_trusted_contact">Contacto de confiança</string><!--Trusted contact-->
    <string name="backup_null_contact">Ainda não adicionou qualquer contacto de confiança.</string><!--You have\'t added any trusted contacts yet.-->
    <string name="backup_warning_title">Social Key Recovery ficará disponível quando tiver pelo menos 3 contactos de confiança (Máx:5).</string><!--Social Key Recovery will be available when you have at least 3 trusted contacts (Max:5).-->
    <string name="backup_invitation_title">Convide amigos para \n contacto de confiança</string><!--Invite friends to be your \n trusted contact-->
    <string name="backup_invitation_content">Convide 3 a 5 amigos ou familiares para contactos de confiança e ajudar a recuperar a sua carteira.</string><!--Invite 3 to 5 friends or family to be trusted contacts and help recover your wallet.-->
    <string name="backup_invitation_hi">Olá %1$s,</string><!--Hi %1$s,-->
