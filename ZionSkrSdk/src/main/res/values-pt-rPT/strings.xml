    <string name="backup_toolbar_sub_title_no_active_status">Não Activo</string><!--Not active-->
    <string name="backup_toolbar_sub_title_active_status">Ativo</string><!--Active-->
    <string name="backup_invitation_message_first_part">Olá, \n\n Escolhi-te como contacto de confiança para a funcionalidade de Social Key Recovery da carteira Zion. \n Segue as instruções abaixo para \n me ajudar a salvaguardar os meus dados de recuperação.\n\n</string><!--Hi, \n\n I\'ve designated you as my trusted contact for the Zion wallet\'s Social Key Recovery feature. \n Please follow the instructions below to \n help me back up my recovery data.\n\n-->
    <string name="backup_invitation_message_second_part">1) Toque nesta ligação para transferir e instalar Zion:\n</string><!--1) Tap this link to download and install Zion:\n-->
    <string name="backup_invitation_message_third_part">2) Quando terminar, não se esqueça de \n me pedir o código de verificação.\n\n</string><!--2) When you\'re done, remember to \n ask me for the verification code.\n\n-->
    <string name="backup_invitation_message_fourth_part">Obrigado!</string><!--Thanks!-->
    <string name="backup_trust_item_sub">Hora da última verificação: %1$s</string><!--Last checked time: %1$s-->
    <string name="backup_trust_item_sub2">Toque aqui para gerar código.</string><!--Tap here to generate code.-->
    <string name="backup_resend_button_title">Reenviar</string><!--Resend-->
    <string name="backup_bottom_request_show">Pedidos de amigos</string><!--Requests from friends-->
    <string name="backup_remove_toolbar_title">Remover contacto de confiança</string><!--Remove trusted contact-->
    <string name="backup_remove_button_delete">Remover(%1$d)</string><!--Remove(%1$d)-->
