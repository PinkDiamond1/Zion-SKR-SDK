<!--Verification UI Start-->
    <string name="ver_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_content_title">Oseba %1$s vas je dodala kot zaupanja vreden stik</string><!--%1$s added you as a trusted contact-->
    <string name="ver_step1">1. korak. Vnesite svoje ime in pridobite potrditveno kodo od osebe %1$s.</string><!--Step 1. Enter your name and get the verification code from %1$s.--> 
    <string name="ver_step2">2. korak. Vaša potrditvena koda je ustvarjena v telefonu osebe %1$s. Prosite jo, da vam pošlje kodo, nato pa jo vnesite.</string> 
    <string name="ver_enter_name_hint">Vnesite svoje ime</string><!--Enter your name--> 
    <string name="ver_request_btn_text">Zahtevaj kodo</string><!--Request code-->
    <string name="ver_sharing_title">Souporabi to kodo z osebo %1$s</string><!--Share this code to %1$s-->
    <string name="ver_sharing_remind_route">M: %1$s\nprek %2$s</string><!--M: %1$s\nvia %1$s-->
    <string name="ver_sharing_reject">ZAVRNI</string><!--REJECT-->
    <string name="ver_notification_channel_name">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_notification_request_title">Nov zahtevek od osebe %1$s</string><!--New request from %1$s--> 
    <string name="ver_notification_request_content">Tapnite, da prikažete potrditveno kodo in jo souporabite z osebo %1$s.</string><!--Tap to see the verification code and share it with %1$s.-->
    <string name="ver_notification_request_button">POKAŽI POTRDITVENO KODO</string><!--SHOW VERIFICATION CODE-->
    <string name="ver_no_response_hint">Ni odziva? Čez %1$s lahko izdate drugo zahtevo</string>
    <string name="ver_verify">Preveri</string><!--Verify-->
    <string name="ver_ok_congratulation_title">Zdaj ste zaupanja vreden stik za osebo %1$s.</string><!--You are now a trusted contact for %1$s.--> 
    <string name="ver_ok_congratulation_msg">Osebi %1$s lahko zdaj pomagate obnoviti denarnico, če jo izgubi ali ji jo ukradejo. Aplikacija Zion je del podatkov za obnovitev denarnice šifrirala in shranila v vašo napravo. Pazite, da ne izbrišete aplikacije Zion ali njenih podatkov, sicer bodo ti podatki za obnovitev izgubljeni. Aplikacija Zion bo pri %2$s občasno preverila, ali so podatki za obnovitev še vedno veljavni.</string><!--You can now help %1$s recover their wallet if it is lost or stolen. Zion has encrypted and saved part of their wallet recovery data to your device. Make sure not to delete Zion or Zion app data, otherwise this recovery data will be lost. Zion will check with %1$s periodically to verify the recovery data is still valid.-->
    <string name="ver_ok_done">Končano</string><!--Done-->
    <string name="ver_pin_error_hint">Ta koda ni veljavna. Poskusite znova.</string><!--This code is invalid, please try again.-->
    <string name="ver_dialog_btn_ok">V REDU</string><!--OK-->
    <string name="ver_dialog_backup_done_title">Ste že zaupanja vreden stik osebe %1$s</string><!--You are already %1$s’s trusted contact-->
    <string name="ver_dialog_backup_done_msg">Hvala za pomoč pri varnostnem kopiranju podatkov osebe %1$s</string><!--Thanks for helping back up %1$s’s data-->
    <string name="ver_dialog_verifying_title">Preverjanje je bilo poslano</string><!--Verification sent-->
    <string name="ver_dialog_verifying">Čakanje na preverjanje. Ko bo preverjanje končano, boste zaupanja vreden stik osebe %1$s.</string><!--Waiting for verifying. You will be %1$s’s trusted contact when verification is completed.-->
    <string name="ver_unknown_phone_number">Neznano</string><!--Unknown-->
    <string name="ver_unknown_name">Neznano</string><!--Unknown-->
    <string name="ver_dialog_trusted_contact_full_title">Hvala za pomoč!</string><!--Thank you for helping out!-->
    <string name="ver_dialog_trusted_contact_full_content">Oseba %1$s že ima največ pet zaupanja vrednih stikov.</string><!--%1$s has already reached the maximum of 5 trusted contacts.-->
    <string name="ver_expired_link">Povezava je potekla. Zaprosite osebo %1$s, da pošlje posodobljeno povezavo.</string><!--This link is expired, please ask %1$s to send the updated link.-->
    <string name="ver_request_timeout">Časovna omejitev zahteve. Poskusite znova pozneje.</string><!--Request timeout, please try again later.-->
