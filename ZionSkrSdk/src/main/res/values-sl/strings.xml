    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">Kaj je Social Key Recovery\?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Storitev Social Key Recovery vam lahko pomaga priročno obnoviti denarnico z zaupanja vrednimi stiki ali družinskimi člani.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Povabite tri do pet oseb kot zaupanja vredne stike, ki vam bodo pomagali obnoviti denarnico.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Vaši zaupanja vredni stiki lahko pošljejo kodo, ki vam bo v pomoč pri obnovitvi denarnice.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Nastavi</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Ne zdaj</a></string>
    <string name="backup_introduction_ok">V REDU</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Varnostno kopirajte seznam zaupanja vrednih stikov</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Če želite obnoviti denarnico s storitvijo Social Key Recovery, izberite Googlov račun za varnostno kopiranje zaupanja vrednih stikov. Seznam zaupanja vrednih stikov bo shranjen v Google Drive.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Dodaj račun</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Prekliči</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Nadaljuj</string><!--Continue-->

<!--Social Key Recovery -->
    <string name="backup_trusted_contact">Zaupanja vreden stik</string><!--Trusted contact-->
    <string name="backup_null_contact">Dodali niste še nobenega zaupanja vrednega stika.</string><!--You have\'t added any trusted contacts yet.-->
    <string name="backup_warning_title">Social Key Recovery bo na voljo, ko boste imeli vsaj tri zaupanja vredne stike (največ pet).</string><!--Social Key Recovery will be available when you have at least 3 trusted contacts (Max:5).-->
    <string name="backup_invitation_title">Povabite prijatelje kot svoje \n zaupanja vredne stike</string><!--Invite friends to be your \n trusted contact-->
    <string name="backup_invitation_content">Povabite tri do pet prijateljev ali družinskih članov kot zaupanja vredne stike, ki vam bodo pomagali obnoviti denarnico.</string><!--Invite 3 to 5 friends or family to be trusted contacts and help recover your wallet.-->
    <string name="backup_invitation_hi">Pozdravljeni, %1$s,</string><!--Hi %1$s,-->
    <string name="backup_invitation_sharing">Souporabi povabilo</string><!--Share invitation-->
    <string name="backup_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="backup_toolbar_sub_title_no_active_status">Ni aktivno</string><!--Not active-->
    <string name="backup_toolbar_sub_title_active_status">Aktivno</string><!--Active-->
    <string name="backup_invitation_message_first_part">Živjo, \n\n določil sem te kot svoj zaupanja vreden stik za funkcijo Social Key Recovery denarnice Zion. \n Prosim, upoštevaj spodnja navodila, da mi pomagaš \n pri varnostnem kopiranju mojih podatkov za obnovitev.\n\n</string><!--Hi, \n\n I\'ve designated you as my trusted contact for the Zion wallet\'s Social Key Recovery feature. \n Please follow the instructions below to \n help me back up my recovery data.\n\n-->
    <string name="backup_invitation_message_second_part">1) Za prenos in namestitev denarnice Zion tapnite to povezavo:\n</string><!--1) Tap this link to download and install Zion:\n-->
    <string name="backup_invitation_message_third_part">2) Ko končate, ne pozabite \n vprašati za kodo za preverjanje.\n\n</string><!--2) When you\'re done, remember to \n ask me for the verification code.\n\n-->
    <string name="backup_invitation_message_fourth_part">Hvala.</string><!--Thanks!-->
    <string name="backup_trust_item_sub">Čas zadnjega preverjanja: %1$s</string><!--Last checked time: %1$s-->
    <string name="backup_trust_item_sub2">Tapnite tukaj, da ustvarite kodo.</string><!--Tap here to generate code.-->
    <string name="backup_resend_button_title">Znova pošlji</string><!--Resend-->
    <string name="backup_bottom_request_show">Zahteve od prijateljev</string><!--Requests from friends-->
    <string name="backup_remove_toolbar_title">Odstrani zaupanja vreden stik</string><!--Remove trusted contact-->
    <string name="backup_remove_button_delete">Odstrani (%1$d)</string><!--Remove(%1$d)-->
    <string name="backup_menu_remove_trust_title">Odstrani zaupanja vreden stik</string><!--Remove trusted contact-->
    <string name="backup_menu_log_out_title">Odjava</string>
    <string name="backup_pending_target_toolbar">Zahteve od prijateljev</string><!--Requests from friends--> 
    <string name="backup_pending_target_null">Trenutno ni nobene zahteve.</string><!--No requests at this time.--> 
    <string name="backup_backup_on_same_device">Varnostno kopiranje v isti napravi ni podprto.</string><!--Back up on same device is not supported.-->

