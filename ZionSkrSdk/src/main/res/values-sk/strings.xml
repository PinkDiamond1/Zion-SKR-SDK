<!--Verification UI Start-->
    <string name="ver_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_content_title">Používateľ %1$s vás pridal ako dôveryhodný kontakt</string><!--%1$s added you as a trusted contact-->
    <string name="ver_step1">1. krok: Zadajte svoje meno a získajte overovací kód od používateľa %1$s.</string><!--Step 1. Enter your name and get the verification code from %1$s.--> 
    <string name="ver_step2">2. krok: Váš overovací kód je vygenerovaný na telefóne používateľa %1$s. Vyžiadajte si od neho kód a zadajte ho.</string> 
    <string name="ver_enter_name_hint">Zadajte svoje meno</string><!--Enter your name--> 
    <string name="ver_request_btn_text">Vyžiadať kód</string><!--Request code-->
    <string name="ver_sharing_title">Zdieľať tento kód s %1$s</string><!--Share this code to %1$s-->
    <string name="ver_sharing_remind_route">M: %1$s\ncez %2$s</string><!--M: %1$s\nvia %1$s-->
    <string name="ver_sharing_reject">ODMIETNUŤ</string><!--REJECT-->
    <string name="ver_notification_channel_name">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_notification_request_title">Nová žiadosť od %1$s</string><!--New request from %1$s--> 
    <string name="ver_notification_request_content">Klepnutím zobrazíte overovací kód a môžete ho zdieľať s používateľom %1$s.</string><!--Tap to see the verification code and share it with %1$s.-->
    <string name="ver_notification_request_button">ZOBRAZIŤ OVEROVACÍ KÓD</string><!--SHOW VERIFICATION CODE-->
    <string name="ver_no_response_hint">Žiadna odpoveď? Môžete požiadať znova o %1$s</string>
    <string name="ver_verify">Overiť</string><!--Verify-->
    <string name="ver_ok_congratulation_title">Teraz ste dôveryhodný kontakt používateľa %1$s.</string><!--You are now a trusted contact for %1$s.--> 
    <string name="ver_ok_congratulation_msg">Používateľovi %1$s môžete pomôcť obnoviť jeho peňaženku v prípade straty alebo odcudzenia. Služba Zion má vo vašom zariadení zašifrovanú a uloženú časť údajov na obnovenie jeho peňaženky. Zaistite, aby ste nedostraňovali aplikáciu Zion ani údaje aplikácie Zion, inak sa údaje na obnovenie stratia. Služba Zion pravidelne u používateľa %2$s skontroluje, či sú údaje na obnovenie stále platné.</string><!--You can now help %1$s recover their wallet if it is lost or stolen. Zion has encrypted and saved part of their wallet recovery data to your device. Make sure not to delete Zion or Zion app data, otherwise this recovery data will be lost. Zion will check with %1$s periodically to verify the recovery data is still valid.-->
    <string name="ver_ok_done">Hotovo</string><!--Done-->
    <string name="ver_pin_error_hint">Tento kód je neplatný, skúste to znova.</string><!--This code is invalid, please try again.-->
    <string name="ver_dialog_btn_ok">OK</string><!--OK-->
    <string name="ver_dialog_backup_done_title">Už ste dôveryhodný kontakt používateľa %1$s</string><!--You are already %1$s’s trusted contact-->
    <string name="ver_dialog_backup_done_msg">Vďaka za pomoc so zálohovaním údajov používateľa %1$s</string><!--Thanks for helping back up %1$s’s data-->
    <string name="ver_dialog_verifying_title">Overenie odoslané</string><!--Verification sent-->
    <string name="ver_dialog_verifying">Čaká sa na overenie. Keď sa overenie dokončí, budete dôveryhodný kontakt pre používateľa %1$s.</string><!--Waiting for verifying. You will be %1$s’s trusted contact when verification is completed.-->
    <string name="ver_unknown_phone_number">Neznáme</string><!--Unknown-->
    <string name="ver_unknown_name">Neznáme</string><!--Unknown-->
    <string name="ver_dialog_trusted_contact_full_title">Ďakujem za pomoc!</string><!--Thank you for helping out!-->
    <string name="ver_dialog_trusted_contact_full_content">%1$s už má dosiahnuté maximum 5 dôveryhodných kontaktov.</string><!--%1$s has already reached the maximum of 5 trusted contacts.-->
    <string name="ver_expired_link">Platnosť odkazu uplynula, požiadajte používateľa %1$s o odoslanie aktualizovaného odkazu.</string><!--This link is expired, please ask %1$s to send the updated link.-->
    <string name="ver_request_timeout">Časový limit siete uplynul, skúste to znova neskôr.</string><!--Request timeout, please try again later.-->
<!--Verification UI End-->

    <string name="try_too_often_title">Skúste to znova neskôr</string><!--Please try again later-->
    <string name="try_too_often_message">Pokúšate sa príliš často, skúste to znova o %1$d min.</string>

    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">Čo je Social Key Recovery\?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery vám pomôže pohodlne obnoviť peňaženku cez dôveryhodných priateľov alebo rodinu.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Pozvite 3 až 5 ľudí, aby boli dôveryhodnými kontaktmi, ktoré vám pomôžu obnoviť peňaženku.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Vaše dôveryhodné kontakty môžu poslať kód a pomôcť vám tak obnoviť vašu peňaženku.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Nastavenie</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Teraz nie</a></string>
    <string name="backup_introduction_ok">OK</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Zálohujte zoznam svojich dôveryhodných kontaktov</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Ak chcete obnoviť svoju peňaženku pomocou funkcie Social Key Recovery, vyberte konto Google, na ktorom ste zálohovali svoje dôveryhodné kontakty. Zoznam dôveryhodných kontaktov bude uložený v službe Disk Google.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Pridať konto</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Zrušiť</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Pokračovať</string><!--Continue-->

<!--Social Key Recovery -->
    <string name="backup_trusted_contact">Dôveryhodný kontakt</string><!--Trusted contact-->
    <string name="backup_null_contact">Zatiaľ nemáte pridaný žiadny dôveryhodný kontakt.</string><!--You have\'t added any trusted contacts yet.-->
    <string name="backup_warning_title">Social Key Recovery bude k dispozícii, keď budete mať aspoň 3 dôveryhodné kontakty (max. 5).</string><!--Social Key Recovery will be available when you have at least 3 trusted contacts (Max:5).-->
    <string name="backup_invitation_title">Pozvite priateľov, aby boli\nvašim dôveryhodným kontaktom</string><!--Invite friends to be your \n trusted contact-->
    <string name="backup_invitation_content">Pozvite 3 až 5 priateľov alebo rodinných príslušníkov, ktorí budú vašimi dôveryhodnými kontaktmi a pomôžu vám s obnovením peňaženky.</string><!--Invite 3 to 5 friends or family to be trusted contacts and help recover your wallet.-->
    <string name="backup_invitation_hi">Ahoj %1$s,</string><!--Hi %1$s,-->
    <string name="backup_invitation_sharing">Zdieľať pozvánku</string><!--Share invitation-->
    <string name="backup_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="backup_toolbar_sub_title_no_active_status">Neaktívny</string><!--Not active-->
    <string name="backup_toolbar_sub_title_active_status">Aktívny</string><!--Active-->
    <string name="backup_invitation_message_first_part">Ahoj,\n\nurčil/a som ťa ako môj dôveryhodný kontakt pre funkciu Social Key Recovery v peňaženke Zion. \nPostupuj podľa pokynov dole\na pomôž mi zálohovať oje údaje na obnovenie.\n\n</string><!--Hi, \n\n I\'ve designated you as my trusted contact for the Zion wallet\'s Social Key Recovery feature. \n Please follow the instructions below to \n help me back up my recovery data.\n\n-->
    <string name="backup_invitation_message_second_part">1) Klepnutím na tento odkaz si prevezmi a nainštaluj aplikáciu Zion:\n</string><!--1) Tap this link to download and install Zion:\n-->
    <string name="backup_invitation_message_third_part">2) Keď skončíš, nezabudni ma\npožiadať o overovací kód.\n\n</string><!--2) When you\'re done, remember to \n ask me for the verification code.\n\n-->
    <string name="backup_invitation_message_fourth_part">Vďaka!</string><!--Thanks!-->
    <string name="backup_trust_item_sub">Naposledy kontrolované: %1$s</string><!--Last checked time: %1$s-->
    <string name="backup_trust_item_sub2">Klepnutím sem vygenerujte kód.</string><!--Tap here to generate code.-->
    <string name="backup_resend_button_title">Poslať znova</string><!--Resend-->
    <string name="backup_bottom_request_show">Žiadosti od priateľov</string><!--Requests from friends-->
    <string name="backup_remove_toolbar_title">Odstrániť dôveryhodný kontakt</string><!--Remove trusted contact-->
    <string name="backup_remove_button_delete">Odstrániť (%1$d)</string><!--Remove(%1$d)-->
    <string name="backup_menu_remove_trust_title">Odstrániť dôveryhodný kontakt</string><!--Remove trusted contact-->
    <string name="backup_menu_log_out_title">Odhlásiť sa</string>
    <string name="backup_pending_target_toolbar">Žiadosti od priateľov</string><!--Requests from friends--> 
    <string name="backup_pending_target_null">Momentálne žiadne žiadosti.</string><!--No requests at this time.--> 
    <string name="backup_backup_on_same_device">Zálohovanie na rovnakom zariadení nie je podporované.</string><!--Back up on same device is not supported.-->

<!--Social Key Recovery(DB (account has been used))-->
    <string name="db_account_used">Toto konto bolo použité na inom zariadení</string><!--This account has been used in another device-->
    <string name="db_security_concern">Z bezpečnostných dôvodov je možné použiť jednotlivé kontá len na jednom zariadení.</string><!--For security concern, each account can be used for one device only.-->
    <string name="db_still_want_to_use_account">Stále chcete použiť toto konto, prípadne použiť iné na spustenie Social Key Recovery?</string><!--Do you still want to use this account or use another one to start Social Key Recovery?-->
