    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">Qu\'est-ce que Social Key Recovery ?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery peut vous aider à récupérer de manière pratique votre portefeuille avec votre famille ou vos amis de confiance.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Invitez 3 à 5 personnes à être contacts de confiance qui peuvent vous aider à récupérer votre portefeuille.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Vos contacts de confiance peuvent vous envoyer un code pour vous aider à restaurer votre portefeuille.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Configurer</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Pas maintenant</a></string>
    <string name="backup_introduction_ok">OK</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Sauvegardez votre liste de contacts de confiance</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Pour récupérer votre portefeuille avec Social Key Recovery, sélectionnez un compte Google pour sauvegarder vos contacts de confiance. Votre liste de contacts de confiance sera stockée dans Google Drive.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Ajouter un compte</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Annuler</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Continuer</string><!--Continue-->

