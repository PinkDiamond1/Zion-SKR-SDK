    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">¿Qué es la Social Key Recovery\?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery te puede ayudar a recuperar tu monedero con la ayuda de amigos o familiares de confianza de una manera fácil.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Invita a 3 a 5 personas a que sean contactos de confianza que te pueden ayudar a recuperar tu monedero.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Tus contactos de confianza pueden enviar un código para ayudarte a recuperar tu monedero.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Configurar</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Ahora no</a></string>
    <string name="backup_introduction_ok">Aceptar</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Realiza una copia de seguridad de la lista de contactos de confianza</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Para recuperar tu monedero con Social Key Recovery, selecciona una cuenta de Google para hacer una copia de seguridad de tus contactos de confianza. Tu lista de contactos de confianza se guardará en Google Drive.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Agregar cuenta</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Cancelar</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Continuar</string><!--Continue-->

<!--Social Key Recovery -->
