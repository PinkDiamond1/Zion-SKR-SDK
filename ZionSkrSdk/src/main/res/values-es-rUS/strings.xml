    <string name="ver_dialog_trusted_contact_full_content">%1$s ya ha alcanzado el máximo de 5 contactos de confianza.</string><!--%1$s has already reached the maximum of 5 trusted contacts.-->
    <string name="ver_expired_link">Este vínculo ha caducado, pide a %1$s que te envíe el vínculo actualizado.</string><!--This link is expired, please ask %1$s to send the updated link.-->
