    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">社交密鑰還原是什麼？</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">社交密鑰還原可幫助您透過您信任的朋友或家人快速還原錢包。</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">邀請 3 到 5 人成為信任的聯絡人，他們可以協助您還原錢包。</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">信任的聯絡人可以傳送代碼以協助您還原錢包。</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">設定</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">現在不要</a></string>
    <string name="backup_introduction_ok">確定</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">備份信任聯絡人清單</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">若要透過社交密鑰還原來還原錢包，請選擇 Google 帳戶以備份信任聯絡人。信任聯絡人清單將儲存於 Google 雲端硬碟。</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">新增帳戶</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">取消</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">繼續</string><!--Continue-->

