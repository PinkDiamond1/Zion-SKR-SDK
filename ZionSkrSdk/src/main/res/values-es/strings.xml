    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">¿Qué es Social Key Recovery\?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery puede ayudarte a recuperar cómodamente tu wallet de tus amigos o familiares de confianza.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Invita de 3 a 5 personas para que sean contactos de confianza que puedan ayudarte a recuperar tu wallet.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Tus contactos de confianza pueden enviar un código para ayudarte a restaurar tu wallet.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Ajustes</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Ahora no</a></string>
    <string name="backup_introduction_ok">Aceptar</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Copia de seguridad de tu lista de contacto de confianza</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Para recuperar tu wallet con Social Key Recovery, selecciona una cuenta de Google para hacer una copia de seguridad de tus contactos de confianza. Tu lista de contactos de confianza se almacenará en Google Drive.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Añadir cuenta</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Cancelar</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Continuar</string><!--Continue-->

<!--Social Key Recovery -->
    <string name="backup_trusted_contact">Contacto de confianza</string><!--Trusted contact-->
    <string name="backup_null_contact">Aún no has añadido ningún contacto de confianza.</string><!--You have\'t added any trusted contacts yet.-->
    <string name="backup_warning_title">Social Key Recovery estará disponible cuando tengas al menos 3 contactos de confianza (Máx:5).</string><!--Social Key Recovery will be available when you have at least 3 trusted contacts (Max:5).-->
    <string name="backup_invitation_title">Invita a tus amigos a ser tu \n contacto de confianza</string><!--Invite friends to be your \n trusted contact-->
    <string name="backup_invitation_content">Invita de 3 a 5 amigos o familiares a ser contactos de confianza y ayudar a recuperar tu wallet.</string><!--Invite 3 to 5 friends or family to be trusted contacts and help recover your wallet.-->
