<!--Verification UI Start-->
    <string name="ver_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_content_title">%1$s te ha añadido como un contacto de confianza</string><!--%1$s added you as a trusted contact-->
    <string name="ver_step1">Paso 1. Introduce tu nombre y obtén el código de verificación de %1$s.</string><!--Step 1. Enter your name and get the verification code from %1$s.-->
    <string name="ver_step2">Paso 2. Tu código de verificación se genera en el teléfono de %1$s. Pídeles el código e introdúcelo.</string>
    <string name="ver_enter_name_hint">Introduce tu nombre</string><!--Enter your name--> 
    <string name="ver_request_btn_text">Solicitar código</string><!--Request code-->
    <string name="ver_sharing_title">Comparte este código con %1$s</string><!--Share this code to %1$s-->
