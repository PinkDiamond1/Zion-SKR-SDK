    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">Mikä on Social Key Recovery\?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery auttaa sinua palauttamaan lompakkosi helposti luotetuilta ystäviltäsi tai perheenjäseniltä.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">Kutsu 3–5 henkilöä luotetuiksi yhteystiedoiksi, jotka voivat auttaa sinua palauttamaan lompakkosi.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Luotetut yhteystietosi voivat lähettää koodin auttamaan sinua palauttamaan lompakkosi.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Määritä asetukset</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Ei nyt</a></string>
    <string name="backup_introduction_ok">OK</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Varmuuskopioi luotettujen yhteystietojesi luettelo</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Palauttaaksesi lompakkosi Social Key Recovery -toiminnolla, valitse Google-tili varmuuskopioidaksesi luotetut yhteystietosi. Luotettujen yhteystietojen luettelosi tallennetaan Google Driveen.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Lisää tili</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">Peruuta</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Jatka</string><!--Continue-->

