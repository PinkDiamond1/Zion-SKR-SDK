    <!-- Social Key Recovery introduction -->
<!-- Zion is name, not to translate Zion-->
    <string name="backup_introduction_title">Social Key Recovery nedir?</string><!--What is Social Key Recovery\?-->
    <string name="backup_introduction_sub_title">Social Key Recovery güvenilir arkadaşlarınız veya aile üyeleriniz aracılığıyla cüzdanınızı kolayca kurtarmanıza yardımcı olabilir.</string><!--Social Key Recovery can help you conveniently recover your wallet from your trusted friends or family.-->
    <string name="backup_introduction_first_warning_title">1.</string>
    <string name="backup_introduction_first_warning">3 ila 5 kişiyi, cüzdanınızı kurtarmaya yardımcı olabilecek güvenilir kişiler olmaya davet edin.</string><!--Invite 3 to 5 people to be trusted contacts who can help recover your wallet.--> 
    <string name="backup_introduction_second_warning_title">2.</string>
    <string name="backup_introduction_second_warning">Güvenilir kişileriniz, cüzdanınızı geri yüklemenize yardımcı olmak için bir kod gönderebilir.</string><!--Your trusted contacts can send a code to help you restore your wallet.-->
    <string name="backup_introduction_setup">Kur</string><!--Set up-->
    <string name="backup_introduction_exit"><a href="com.htc.wallet://">Şimdi değil</a></string>
    <string name="backup_introduction_ok">Tamam</string><!--OK-->
<!-- Social Key Recovery choose google drive -->
    <string name="backup_choose_google_drive_title">Güvenilir kişi listenizi yedekleyin</string><!--Back up your trusted contact list-->
    <string name="backup_choose_google_drive_sub_title">Cüzdanınızı Social Key Recovery işleviyle kurtarmak için, güvenilir kişilerinizi yedeklemek amacıyla bir Google hesabı seçin. Güvenilir kişi listeniz Google Drive\'da saklanacaktır.</string><!--To recover your wallet with Social Key Recovery, select a Google account to back up your trusted contacts. Your trusted contact list will be stored in Google Drive.-->
    <string name="backup_choose_google_drive_add_account">Hesap ekle</string><!--Add account-->
    <string name="backup_choose_google_drive_bottom_cancel_btn">İptal</string><!--Cancel-->
    <string name="backup_choose_google_drive_bottom_continue_btn">Devam</string><!--Continue-->

<!--Social Key Recovery -->
    <string name="backup_trusted_contact">Güvenilir kişi</string><!--Trusted contact-->
    <string name="backup_null_contact">Henüz hiç güvenilir kişi eklemediniz.</string><!--You have\'t added any trusted contacts yet.-->
    <string name="backup_warning_title">Social Key Recovery en az 3 güvenilir kişiye (en fazla 5) sahip olduğunuzda kullanılabilecektir.</string><!--Social Key Recovery will be available when you have at least 3 trusted contacts (Max:5).-->
    <string name="backup_invitation_title">Güvenilir kişileriniz olacak \n arkadaşları davet edin</string><!--Invite friends to be your \n trusted contact-->
    <string name="backup_invitation_content">3 ila 5 arkadaşı veya aile üyesini güvenilir kişiler olmaya ve cüzdanınızı kurtarmaya yardım etmeye davet edin.</string><!--Invite 3 to 5 friends or family to be trusted contacts and help recover your wallet.-->
    <string name="backup_invitation_hi">Merhaba %1$s,</string><!--Hi %1$s,-->
    <string name="backup_invitation_sharing">Davet paylaş</string><!--Share invitation-->
    <string name="backup_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="backup_toolbar_sub_title_no_active_status">Etkin değil</string><!--Not active-->
    <string name="backup_toolbar_sub_title_active_status">Etkin</string><!--Active-->
    <string name="backup_invitation_message_first_part">Merhaba, \n\n Seni Zion cüzdanının Social Key Recovery özelliği için güvenilir kişim olarak atadım. \n Kurtarma verilerimi yedeklemede bana \n yardım etmek için lütfen aşağıdaki yönergeleri izle.\n\n</string><!--Hi, \n\n I\'ve designated you as my trusted contact for the Zion wallet\'s Social Key Recovery feature. \n Please follow the instructions below to \n help me back up my recovery data.\n\n-->
    <string name="backup_invitation_message_second_part">1) Zion uygulamasını indirip yüklemek için şu bağlantıya dokun:\n</string><!--1) Tap this link to download and install Zion:\n-->
    <string name="backup_invitation_message_third_part">2) Tamamladığında, benden \n doğrulama kodunu istemeyi unutma.\n\n</string><!--2) When you\'re done, remember to \n ask me for the verification code.\n\n-->
    <string name="backup_invitation_message_fourth_part">Teşekkürler!</string><!--Thanks!-->
    <string name="backup_trust_item_sub">Son denetleme zamanı: %1$s</string><!--Last checked time: %1$s-->
    <string name="backup_trust_item_sub2">Kod oluşturmak için buraya dokunun.</string><!--Tap here to generate code.-->
    <string name="backup_resend_button_title">Tekrar Gönder</string><!--Resend-->
    <string name="backup_bottom_request_show">Arkadaşlardan istekler</string><!--Requests from friends-->
    <string name="backup_remove_toolbar_title">Güvenilir kişiyi kaldır</string><!--Remove trusted contact-->
    <string name="backup_remove_button_delete">Kaldır (%1$d)</string><!--Remove(%1$d)-->
    <string name="backup_menu_remove_trust_title">Güvenilir kişiyi kaldır</string><!--Remove trusted contact-->
    <string name="backup_menu_log_out_title">Oturumu kapat</string>
    <string name="backup_pending_target_toolbar">Arkadaşlardan istekler</string><!--Requests from friends--> 
    <string name="backup_pending_target_null">Şu anda hiç istek yok.</string><!--No requests at this time.--> 
    <string name="backup_backup_on_same_device">Aynı aygıtta yedekleme desteklenmez.</string><!--Back up on same device is not supported.-->

