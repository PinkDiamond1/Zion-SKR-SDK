    <string name="backup_toolbar_sub_title_no_active_status">Nije aktivan</string><!--Not active-->
    <string name="backup_toolbar_sub_title_active_status">Aktivan</string><!--Active-->
    <string name="backup_invitation_message_first_part">Zdravo, \n\n Imenovao/la sam te mojim pouzdanim kontaktom za korištenje značajke Social Key Recovery u aplikaciji Zion novčanika. \n Slijedite dolje navedene upute kako bi mi \n pomogli sigurnosno pohraniti moje podatke za oporavak.\n\n</string><!--Hi, \n\n I\'ve designated you as my trusted contact for the Zion wallet\'s Social Key Recovery feature. \n Please follow the instructions below to \n help me back up my recovery data.\n\n-->
    <string name="backup_invitation_message_second_part">1) Dodirni ovu poveznicu za preuzimanje i instalaciju aplikacije Zion:\n</string><!--1) Tap this link to download and install Zion:\n-->
    <string name="backup_invitation_message_third_part">2) Nakon toga mi pošalji \n upit za verifikacijski kod.\n\n</string><!--2) When you\'re done, remember to \n ask me for the verification code.\n\n-->
    <string name="backup_invitation_message_fourth_part">Zahvaljujemo!</string><!--Thanks!-->
    <string name="backup_trust_item_sub">Vrijeme zadnje provjere: %1$s</string><!--Last checked time: %1$s-->
    <string name="backup_trust_item_sub2">Dodirnite ovdje i generirajte kod.</string><!--Tap here to generate code.-->
    <string name="backup_resend_button_title">Pošalji ponovno</string><!--Resend-->
    <string name="backup_bottom_request_show">Zahtjevi od prijatelja</string><!--Requests from friends-->
    <string name="backup_remove_toolbar_title">Uklonite pouzdani kontakt</string><!--Remove trusted contact-->
