<!--Verification UI Start-->
    <string name="ver_toolbar_title">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_content_title">%1$s vas je dodao(la kao pouzdani kontakt</string><!--%1$s added you as a trusted contact-->
    <string name="ver_step1">Korak 1. Unesite svoje ime i zatražite verifikacijski kod od osobe %1$s.</string><!--Step 1. Enter your name and get the verification code from %1$s.-->
    <string name="ver_step2">Korak 2. Vaš verifikacijski kod je generiran na telefonu osobe %1$s. Zatražite taj kod i unesite ga.</string>
    <string name="ver_enter_name_hint">Unesite svoje ime</string><!--Enter your name--> 
    <string name="ver_request_btn_text">Zatražite kod</string><!--Request code-->
    <string name="ver_sharing_title">Podijeli ovaj kod s %1$s</string><!--Share this code to %1$s-->
    <string name="ver_sharing_remind_route">M: %1$s\nputem %2$s</string><!--M: %1$s\nvia %1$s-->
    <string name="ver_sharing_reject">ODBACI</string><!--REJECT-->
    <string name="ver_notification_channel_name">Social Key Recovery</string><!--Social Key Recovery-->
    <string name="ver_notification_request_title">Novi zahtjev od %1$s</string><!--New request from %1$s-->
    <string name="ver_notification_request_content">Dodirnite kako biste vidjeli verifikacijski kod i podijelili ga s %1$s.</string><!--Tap to see the verification code and share it with %1$s.-->
    <string name="ver_notification_request_button">PRIKAŽI VERIFIKACIJSKI KOD</string><!--SHOW VERIFICATION CODE-->
    <string name="ver_no_response_hint">Nema odgovora? Drugi zahtjev možete poslati za %1$s</string>
    <string name="ver_verify">Potvrdi</string><!--Verify-->
    <string name="ver_ok_congratulation_title">Sada ste pouzdani kontakt osobe %1$s.</string><!--You are now a trusted contact for %1$s.-->
    <string name="ver_ok_congratulation_msg">Sada možete pomoći da %1$s oporavi svoj novčanik ako je izgubljen ili ukraden. Zion je šifrirao i pohranio dio podataka za oporavak novčanika na vašem uređaju. Pazite kako ne biste izbrisali aplikaciju Zion ili podatke aplikacije Zion jer u suprotnom ćete izgubiti podatke za oporavak. Zion će povremeno provjeriti kod osobe %2$s jesu li podaci za oporavak još uvijek valjani.</string><!--You can now help %1$s recover their wallet if it is lost or stolen. Zion has encrypted and saved part of their wallet recovery data to your device. Make sure not to delete Zion or Zion app data, otherwise this recovery data will be lost. Zion will check with %1$s periodically to verify the recovery data is still valid.-->
    <string name="ver_ok_done">Gotovo</string><!--Done-->
    <string name="ver_pin_error_hint">Ovaj kod je neispravan, pokušajte ponovno.</string><!--This code is invalid, please try again.-->
    <string name="ver_dialog_btn_ok">U redu</string><!--OK-->
    <string name="ver_dialog_backup_done_title">Već ste pouzdani kontakt osobe %1$s</string><!--You are already %1$s’s trusted contact-->
    <string name="ver_dialog_backup_done_msg">Hvala na pomoći za sigurnosnu pohranu podataka osobe %1$s</string><!--Thanks for helping back up %1$s’s data-->
    <string name="ver_dialog_verifying_title">Verifikacija poslana</string><!--Verification sent-->
    <string name="ver_dialog_verifying">Čeka se verifikacija. Postat ćete pouzdani kontakt osobe %1$s nakon završetka postupka verifikacije.</string><!--Waiting for verifying. You will be %1$s’s trusted contact when verification is completed.-->
    <string name="ver_unknown_phone_number">Nepoznato</string><!--Unknown-->
    <string name="ver_unknown_name">Nepoznato</string><!--Unknown-->
    <string name="ver_dialog_trusted_contact_full_title">Hvala za pomoć!</string><!--Thank you for helping out!-->
    <string name="ver_dialog_trusted_contact_full_content">%1$s je već dostigao/la maksimum od 5 pouzdanih kontakata.</string><!--%1$s has already reached the maximum of 5 trusted contacts.-->
    <string name="ver_expired_link">Ova poveznica je istekla, zamolite osobu %1$s neka vam pošalje ažurnu poveznicu.</string><!--This link is expired, please ask %1$s to send the updated link.-->
    <string name="ver_request_timeout">Vrijeme zahtjeva je isteklo, pokušajte ponovno kasnije.</string><!--Request timeout, please try again later.-->
